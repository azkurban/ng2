  "scripts": {
    "build": "npm run clean:dist && ngc -p tsconfig.prod.json && gulp copy:assets && npm run copy:dist && npm run bundle && npm run bundle:min",
    "bundle": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "bundle:min": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "clean:dist": "npm run rimraf -- dist && npm run rimraf -- dll",
    "copy:dist": "node scripts/package package.json dist && node scripts/copy core dist && node scripts/copy plugin dist",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "github-deploy": "webpack --config config/webpack.github-deploy.js --progress --profile --env.githubProd",
    "lint": "tslint core/**/*.d.ts",
    "release": "git stash && git checkout 1.0.x && git pull origin master && npm run test && npm run build && cd dist && npm publish && cd ..",
    "rimraf": "rimraf",
    "start": "webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch --content-base demo/",
    "test": "npm run lint",
    "test:watch": "npm run test -- --auto-watch --no-single-run"
  },
